{
    "header-settings" : {
        "type" : "header",
        "description" : "Settings"
    },
    "country": {
        "type": "combobox",
        "default": "",
        "description": "Specify country to use in quick-connect",
        "options": {
            "None": ""
        }
    },
    "autoconnect": {
        "type": "switch",
        "default": false,
        "description": "Autoconnect (not sure what this does)"
    },
    "killswitch": {
        "type": "switch",
        "default": false,
        "description": "Internet killswitch when VPN connection drops"
    },
    "cybersec": {
        "type": "switch",
        "default": false,
        "description": "CyberSec: Additional advertising, privacy, and security protection"
    },
    "protocol": {
        "type": "radiogroup",
        "default": "udp",
        "description": "VPN protocol",
        "options": {
            "UDP": "udp",
            "TCP": "tcp"
        }
    },
    "notifications": {
        "type": "switch",
        "default": true,
        "description": "Notifications"
    },
    "login/out":{
        "type": "separator"
    },
    "login":{
        "type": "button",
        "description": "Log in",
        "callback": "login"
    },
    "logout":{
        "type": "button",
        "description": "Log out",
        "callback": "logout"
    }
}
