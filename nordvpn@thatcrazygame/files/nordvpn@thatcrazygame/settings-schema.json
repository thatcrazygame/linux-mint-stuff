            ]
        },
        "section2": {
            "type": "section",
            "title": "DNS Servers",
            "keys": ["ip-0","ip-1","ip-2","ip-3","ip-4","ip-5"],
            "dependency": "dns=true"
        },
        "section3": {
            "type": "section",
            "title": "General Settings",
            "keys": ["autoconnect","notifications","login","logout"]
        }
    },

    "protocol": {
        "type": "radiogroup",
        "default": "udp",
        "description": "Protocol",
        "options": {
            "UDP": "udp",
            "TCP": "tcp"
        }
    },
    "killswitch": {
        "type": "switch",
        "default": false,
        "description": "Kill Switch",
        "tooltip": "Internet killswitch when VPN connection drops"
    },
    "cybersec": {
        "type": "switch",
        "default": false,
        "description": "CyberSec",
        "tooltip": "Additional advertising, privacy, and security protection"
    },
    "obfuscate": {
        "type": "switch",
        "default": false,
        "description": "Obfuscated Servers",
        "tooltip": "Servers capable of bypassing specific country restrictions"
    },
    "dns": {
        "type": "switch",
        "default": false,
        "description": "DNS"
    },
